<h1></h1>

  <%= %>

  <div class="panel panel-default">
    <div class="panel-body">
      <h1 class="text-center">Student Record</h1>
    </div>
  </div>
  <table class="table table-striped table-hover">
    <thead>
      <tr class="info">
        <th>Date</th>
        <th>Attendance</th>
        <th>Participation</th>
        <th>Homework</th>
        <th>Quizzes</th>
        <th>Comments</th>
        <th>Midterm/Final</th>
      </tr>
    </thead>
    <tbody>
      <% @daily_grades.each do |grade| %>
        <tr>
          <td></td>
          <td><%= grade.attendance %></td>
          <td><%= grade.participation %></td>
          <td><%= grade.homework %></td>
          <td><%= grade.quiz %></td>
          <td><span class="glyphicon glyphicon-ok" aria-hidden="true"></span></td>
          <td><%= grade.exam %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
